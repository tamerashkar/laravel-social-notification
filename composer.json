{
    "name": "tashkar18/notification",
    "description": "Simple Notification System for Laravel 4.2",
    "authors": [
        {
            "name": "Tamer Ashkar",
            "email": "tashkar18@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "illuminate/database": "4.2.*",
        "illuminate/view": "4.2.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Tashkar18\\Notification\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
